{
  "name": "typescript-action",
  "version": "0.0.1",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "build": "swc --delete-dir-on-start --extensions .ts src -d lib",
    "format:check": "biome format src && biome format __tests__",
    "format:fix": "biome format --write src && biome format --write __tests__",
    "style:check": "biome check src && biome check __tests__",
    "style:fix": "biome check --write --unsafe src",
    "lint": "pnpm run --parallel /.*:check/",
    "lint-fix": "pnpm run /.*:fix/",
    "package": "ncc build --source-map --license licenses.txt",
    "pretest": "pnpm build",
    "test": "jest",
    "all": "pnpm build && pnpm lint-fix && pnpm package && pnpm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@aws-sdk/client-ssm": "^3.654.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.2",
    "@jest/globals": "^29.7.0",
    "@swc/cli": "^0.1.65",
    "@swc/core": "^1.7.26",
    "@swc/helpers": "^0.5.13",
    "@swc/jest": "^0.2.36",
    "@types/node": "^20.16.6",
    "@vercel/ncc": "^0.38.2",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "regenerator-runtime": "^0.14.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.2"
  },
  "engines": {
    "npm": "use pnpm please!",
    "yarn": "use pnpm please!",
    "pnpm": ">=8.7.1"
  },
  "packageManager": "pnpm@9.11.0+sha512.0a203ffaed5a3f63242cd064c8fb5892366c103e328079318f78062f24ea8c9d50bc6a47aa3567cabefd824d170e78fa2745ed1f16b132e16436146b7688f19b"
}
