{
  "name": "typescript-action",
  "version": "0.0.1",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "build": "swc --delete-dir-on-start --extensions .ts src -d lib",
    "format-check": "rome format src __tests__",
    "format": "rome format --write src __tests__",
    "check": "rome check src __tests__",
    "check-fix": "rome check --apply-suggested src __tests__",
    "lint": "yarn format-check && yarn check",
    "lint-fix": "yarn format && yarn check-fix",
    "package": "ncc build --source-map --license licenses.txt",
    "pretest": "yarn build",
    "test": "jest",
    "all": "yarn build && yarn format && yarn lint && yarn package && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "*",
    "@aws-sdk/client-ssm": "*",
    "source-map-support": "*"
  },
  "devDependencies": {
    "@jest/globals": "*",
    "@swc/cli": "*",
    "@swc/core": "*",
    "@swc/helpers": "*",
    "@swc/jest": "*",
    "@types/node": "*",
    "@vercel/ncc": "*",
    "jest": "*",
    "js-yaml": "*",
    "regenerator-runtime": "*",
    "rome": "*",
    "ts-jest": "*",
    "typescript": "*"
  },
  "resolutions": {
    "semver-regex": ">=3.1.3",
    "got": ">=11.8.5"
  }
}
