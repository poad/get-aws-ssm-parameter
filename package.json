{
  "name": "typescript-action",
  "version": "0.0.1",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "build": "swc --delete-dir-on-start --extensions .ts src -d lib",
    "format:check": "biome format src && biome format __tests__",
    "format:fix": "biome format --write src && biome format --write __tests__",
    "style:check": "biome check src && biome check __tests__",
    "style:fix": "biome check --apply-unsafe src",
    "lint": "pnpm run --parallel /.*:check/",
    "lint-fix": "pnpm run /.*:fix/",
    "package": "ncc build --source-map --license licenses.txt",
    "pretest": "pnpm build",
    "test": "jest",
    "all": "pnpm build && pnpm lint-fix && pnpm package && pnpm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@aws-sdk/client-ssm": "^3.587.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.3",
    "@jest/globals": "^29.7.0",
    "@swc/cli": "^0.1.65",
    "@swc/core": "^1.5.24",
    "@swc/helpers": "^0.5.11",
    "@swc/jest": "^0.2.36",
    "@types/node": "^20.12.13",
    "@vercel/ncc": "^0.38.1",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "regenerator-runtime": "^0.14.1",
    "ts-jest": "^29.1.4",
    "typescript": "^5.4.5"
  },
  "engines": {
    "npm": "use pnpm please!",
    "yarn": "use pnpm please!",
    "pnpm": ">=8.7.1"
  },
  "packageManager": "pnpm@9.1.4+sha512.9df9cf27c91715646c7d675d1c9c8e41f6fce88246f1318c1aa6a1ed1aeb3c4f032fcdf4ba63cc69c4fe6d634279176b5358727d8f2cc1e65b65f43ce2f8bfb0"
}
