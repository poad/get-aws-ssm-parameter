    "@aws-sdk/client-ssm": "^3.927.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@stylistic/eslint-plugin": "^5.5.0",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.0",
    "@swc/helpers": "^0.5.17",
    "@types/node": "^24.10.0",
    "@vercel/ncc": "^0.38.4",
    "eslint": "^9.39.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
