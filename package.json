  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@stylistic/eslint-plugin": "^5.3.1",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.5",
    "@swc/helpers": "^0.5.17",
    "@types/node": "^24.5.2",
    "@vercel/ncc": "^0.38.4",
    "eslint": "^9.35.0",
