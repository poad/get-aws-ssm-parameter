{
  "name": "typescript-action",
  "version": "0.0.1",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "build": "swc --delete-dir-on-start --extensions .ts src -d lib",
    "format-check": "rome format src && rome format __tests__",
    "format": "rome format --write src && rome format --write __tests__",
    "check": "rome check src && rome check __tests__",
    "check-fix": "rome check --apply-unsafe src && rome check --apply-unsafe __tests__",
    "lint": "pnpm format-check && pnpm check",
    "lint-fix": "pnpm format && pnpm check-fix",
    "package": "ncc build --source-map --license licenses.txt",
    "pretest": "pnpm build",
    "test": "jest",
    "all": "pnpm build && pnpm format && pnpm lint && pnpm package && pnpm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@aws-sdk/client-ssm": "^3.362.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.67",
    "@swc/helpers": "^0.5.1",
    "@swc/jest": "^0.2.26",
    "@types/node": "^20.3.2",
    "@vercel/ncc": "^0.36.1",
    "jest": "^29.5.0",
    "js-yaml": "^4.1.0",
    "regenerator-runtime": "^0.13.11",
    "rome": "^12.1.3",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.6"
  }
}
