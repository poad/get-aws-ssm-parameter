#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ken-yo/git/poad/get-aws-ssm-parameter/node_modules/.pnpm/metro@0.73.10/node_modules/metro/src/node_modules:/Users/ken-yo/git/poad/get-aws-ssm-parameter/node_modules/.pnpm/metro@0.73.10/node_modules/metro/node_modules:/Users/ken-yo/git/poad/get-aws-ssm-parameter/node_modules/.pnpm/metro@0.73.10/node_modules:/Users/ken-yo/git/poad/get-aws-ssm-parameter/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ken-yo/git/poad/get-aws-ssm-parameter/node_modules/.pnpm/metro@0.73.10/node_modules/metro/src/node_modules:/Users/ken-yo/git/poad/get-aws-ssm-parameter/node_modules/.pnpm/metro@0.73.10/node_modules/metro/node_modules:/Users/ken-yo/git/poad/get-aws-ssm-parameter/node_modules/.pnpm/metro@0.73.10/node_modules:/Users/ken-yo/git/poad/get-aws-ssm-parameter/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../metro@0.73.10/node_modules/metro/src/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../metro@0.73.10/node_modules/metro/src/cli.js" "$@"
fi
