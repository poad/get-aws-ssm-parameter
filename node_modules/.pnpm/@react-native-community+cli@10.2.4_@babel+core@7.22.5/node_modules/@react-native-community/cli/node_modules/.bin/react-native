#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ken-yo/git/poad/get-aws-ssm-parameter/node_modules/.pnpm/@react-native-community+cli@10.2.4_@babel+core@7.22.5/node_modules/@react-native-community/cli/build/node_modules:/Users/ken-yo/git/poad/get-aws-ssm-parameter/node_modules/.pnpm/@react-native-community+cli@10.2.4_@babel+core@7.22.5/node_modules/@react-native-community/cli/node_modules:/Users/ken-yo/git/poad/get-aws-ssm-parameter/node_modules/.pnpm/@react-native-community+cli@10.2.4_@babel+core@7.22.5/node_modules/@react-native-community/node_modules:/Users/ken-yo/git/poad/get-aws-ssm-parameter/node_modules/.pnpm/@react-native-community+cli@10.2.4_@babel+core@7.22.5/node_modules:/Users/ken-yo/git/poad/get-aws-ssm-parameter/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ken-yo/git/poad/get-aws-ssm-parameter/node_modules/.pnpm/@react-native-community+cli@10.2.4_@babel+core@7.22.5/node_modules/@react-native-community/cli/build/node_modules:/Users/ken-yo/git/poad/get-aws-ssm-parameter/node_modules/.pnpm/@react-native-community+cli@10.2.4_@babel+core@7.22.5/node_modules/@react-native-community/cli/node_modules:/Users/ken-yo/git/poad/get-aws-ssm-parameter/node_modules/.pnpm/@react-native-community+cli@10.2.4_@babel+core@7.22.5/node_modules/@react-native-community/node_modules:/Users/ken-yo/git/poad/get-aws-ssm-parameter/node_modules/.pnpm/@react-native-community+cli@10.2.4_@babel+core@7.22.5/node_modules:/Users/ken-yo/git/poad/get-aws-ssm-parameter/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../build/bin.js" "$@"
else
  exec node  "$basedir/../../build/bin.js" "$@"
fi
