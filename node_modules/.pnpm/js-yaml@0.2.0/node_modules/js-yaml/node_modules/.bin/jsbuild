#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ken-yo/git/poad/get-aws-ssm-parameter/node_modules/.pnpm/jsclass@3.0.4/node_modules/jsclass/bin/node_modules:/Users/ken-yo/git/poad/get-aws-ssm-parameter/node_modules/.pnpm/jsclass@3.0.4/node_modules/jsclass/node_modules:/Users/ken-yo/git/poad/get-aws-ssm-parameter/node_modules/.pnpm/jsclass@3.0.4/node_modules:/Users/ken-yo/git/poad/get-aws-ssm-parameter/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ken-yo/git/poad/get-aws-ssm-parameter/node_modules/.pnpm/jsclass@3.0.4/node_modules/jsclass/bin/node_modules:/Users/ken-yo/git/poad/get-aws-ssm-parameter/node_modules/.pnpm/jsclass@3.0.4/node_modules/jsclass/node_modules:/Users/ken-yo/git/poad/get-aws-ssm-parameter/node_modules/.pnpm/jsclass@3.0.4/node_modules:/Users/ken-yo/git/poad/get-aws-ssm-parameter/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../jsclass/bin/jsbuild" "$@"
else
  exec node  "$basedir/../../../jsclass/bin/jsbuild" "$@"
fi
